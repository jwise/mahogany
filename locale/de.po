msgstr "&Schrift-Familie:Standard:Dekorativ:Roman:Skript:Schweizer:Modern:Schreibmaschine"

#: gui/wxOptionsDlg.cpp:1337
msgid "Colour for header &names"
msgstr "Farbe für &Kopfzeilennamen"

#: gui/wxOptionsDlg.cpp:1338
msgid "Colour for header &values"
msgstr "Farbe für Kopfzeilen&einträge"

#: gui/wxOptionsDlg.cpp:1339
msgid "&Highlight the URLs"
msgstr "URLs hervorheben"

#: gui/wxOptionsDlg.cpp:1340
